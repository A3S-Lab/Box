{
  "name": "@a3s-lab/box",
  "version": "0.1.0",
  "description": "A3S Box - Meta-Agent Sandbox Based on MicroVMs",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./native": {
      "import": "./index.js",
      "types": "./index.d.ts"
    }
  },
  "files": [
    "dist",
    "proto",
    "index.js",
    "index.d.ts",
    "*.node"
  ],
  "napi": {
    "name": "a3s-lab-box",
    "triples": {
      "defaults": true,
      "additional": [
        "x86_64-unknown-linux-gnu",
        "aarch64-unknown-linux-gnu",
        "x86_64-apple-darwin",
        "aarch64-apple-darwin"
      ]
    }
  },
  "scripts": {
    "build": "npm run build:ts && npm run build:native",
    "build:ts": "tsc",
    "build:native": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build && napi prepublish -t npm",
    "version": "napi version"
  },
  "keywords": [
    "a3s",
    "box",
    "microvm",
    "agent",
    "sandbox",
    "grpc",
    "ai"
  ],
  "author": "A3S Team",
  "license": "MIT",
  "dependencies": {
    "@grpc/grpc-js": "^1.12.0",
    "@grpc/proto-loader": "^0.7.0"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.14.0",
    "@types/node": "^20.11.0",
    "typescript": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/A3S-Lab/Box"
  },
  "engines": {
    "node": ">= 18"
  }
}
